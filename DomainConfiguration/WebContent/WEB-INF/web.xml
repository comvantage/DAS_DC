<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DomainConfiguration</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Apache CXF Endpoint</description>
    <display-name>cxf</display-name>
    <servlet-name>cxf</servlet-name>
    <servlet-class>org.apache.cxf.transport.servlet.CXFServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>cxf</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>WEB-INF/beans.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>DomainSourceMapAdminServlet</display-name>
    <servlet-name>DomainSourceMapAdminServlet</servlet-name>
    <servlet-class>eu.comvantage.domainconfiguration.DomainSourceMapAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DomainSourceMapAdminServlet</servlet-name>
    <url-pattern>/DomainSourceMapAdminServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DomainSystemParameterAdminServlet</display-name>
    <servlet-name>DomainSystemParameterAdminServlet</servlet-name>
    <servlet-class>eu.comvantage.domainconfiguration.DomainSystemParameterAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DomainSystemParameterAdminServlet</servlet-name>
    <url-pattern>/DomainSystemParameterAdminServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DomainSPARULTemplatesAdminServlet</display-name>
    <servlet-name>DomainSPARULTemplatesAdminServlet</servlet-name>
    <servlet-class>eu.comvantage.domainconfiguration.DomainSPARULTemplatesAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DomainSPARULTemplatesAdminServlet</servlet-name>
    <url-pattern>/DomainSPARULTemplatesAdminServlet/*</url-pattern>
  </servlet-mapping>
</web-app>